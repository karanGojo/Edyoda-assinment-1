    "a = input(\"Enter User input:\")# user input\n",
    "b = \"\"\n",
    "i =len(a) - 1# length function\n",
    "while (i>=0):\n",
    "    b= b + a[i]\n",
    "    i= i -1\n",
    "print(\"Original string Entered:\",end=\"  \")\n",
    "print(a)\n",
    "print(\"Reversed string or inverted string:\",end=\"  \")\n",
    "print(b)"
